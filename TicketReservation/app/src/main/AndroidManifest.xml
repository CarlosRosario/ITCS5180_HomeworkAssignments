<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group26.ticketreservation">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateTicketActivity"
            android:label="@string/create_ticket_title" />
        <activity
            android:name=".EditTicketActivity"
            android:label="@string/edit_ticket_title" />
        <activity
            android:name=".DeleteTicketActivity"
            android:label="@string/delete_ticket_title" />
        <activity android:name=".ViewTicketActivity" />
        <activity android:name=".PrintTicketActivity"></activity>
    </application>

</manifest>
